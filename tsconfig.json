{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2017",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "strict": true,
    "lib": ["ES2020", "DOM"],
    "outDir": "./built",
    "allowJs": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "types": ["vite/client"]
  },
  "include": ["./src/**/*"]
}
